
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


@model List<Kategori>


<table class="table">
    <h1> <a asp-controller="Kategori" asp-action="Kategori_Ekle">Yeni Kategori Ekle</a></h1>
    <h1> <a asp-controller="Kategori" asp-action="Ürünlere_Git">Ürünler Sayfasına Git</a></h1>
    <thead>
        <tr>
            <th>İsim</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.İsim</td>

            </tr>
        }
    </tbody>
</table>
